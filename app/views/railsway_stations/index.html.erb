<p id="notice"><%= notice %></p>

<h1>Railsway Stations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @railsway_stations.each do |railsway_station| %>
      <tr>
        <td><%= railsway_station.title %></td>
        <td><%= link_to 'Show', railsway_station %></td>
        <td><%= link_to 'Edit', edit_railsway_station_path(railsway_station) %></td>
        <td><%= link_to 'Destroy', railsway_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Railsway Station', new_railsway_station_path %>
