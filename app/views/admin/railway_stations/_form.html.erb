<%= form_for([:admin,  @railway_station]) do |form| %>
<%= render 'common/errors', resource: @railway_station %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title, id: :railway_station_title %>
  </div>

  <div class="actions">
    <% if @railway_station.new_record? %>
      <%= form.submit t('common.submit') %>
    <% else %>
      <%= form.submit t('common.update') %>
    <% end %>
  </div>
<% end %>
